    "update-dependencies": "pnpx npm-check-updates@latest -u"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@adama/tsconfig": "workspace:*",
    "eslint-config-adama": "workspace:*",
    "@types/node": "^20.4.1",
    "eslint": "^8.44.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "@vitest/coverage-v8": "^0.33.0",
    "vitest": "^0.33.0"
  }
}